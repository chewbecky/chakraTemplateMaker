{
  "name": "chakrathememaker",
  "version": "0.1.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "chakraThemeMaker",
    "manifest": "src/manifest.json",
    "main": "chakrathememaker.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.4",
    "@skpm/extract-loader": "^2.0.2",
    "@types/node": "^14.14.27",
    "cocoascript-types": "^0.0.8",
    "css-loader": "^3.2.0",
    "html-loader": "^0.5.5",
    "prettier": "^2.2.1"
  },
  "resources": [
    "resources/**/*.js"
  ],
  "dependencies": {
    "@andrewstart/sketch.d.ts": "^0.0.5",
    "@chakra-ui/react": "^1.3.3",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "framer-motion": "^3.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-query": "^3.9.3",
    "sketch-module-web-view": "^3.1.4",
    "sketch-types": "^0.4.3",
    "ts-loader": "^8.0.17",
    "typescript": "^4.1.5"
  },
  "author": "chewbecca <beckybettinger@yahoo.de>"
}
